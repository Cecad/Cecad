{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Paquete</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Categoria</th>
                <td>{{ entity.categoria }}</td>
            </tr>
            <tr>
                <th>Ram</th>
                <td>{{ entity.ram }}</td>
            </tr>
            <tr>
                <th>Procesador</th>
                <td>{{ entity.procesador }}</td>
            </tr>
            <tr>
                <th>Almacenamiento</th>
                <td>{{ entity.almacenamiento }}</td>
            </tr>
            <tr>
                <th>Precio</th>
                <td>{{ entity.precio }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_paquete') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_paquete_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
