{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Core</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Sistema Operativo</th>
                <td>{{ entity.sistemaOperativo }}</td>
            </tr>
            <tr>
                <th>Programas Instalados</th>
                <td>{{ entity.programasInstalados }}</td>
            </tr>
            <tr>
                <th>Caracteristicas</th>
                <td>
		            <ul>
		            	<li>Nucleos: {{ entity.paquete.procesador }}</li>
		            	<li>RAM: {{ entity.paquete.ram }} GB</li>
		            	<li>Almacenamiento: {{ entity.paquete.almacenamiento }} GB</li>
		            </ul>
                </td>
            </tr>
            <tr>
                <th>Usuario Asigna</th>
                <td>{{ entity.usuarioAprueba }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('usuario_proyecto_show', {'id': entity.proyecto.id}) }}">
            Volver al proyecto
        </a>
    </li>
</ul>
{% endblock %}
